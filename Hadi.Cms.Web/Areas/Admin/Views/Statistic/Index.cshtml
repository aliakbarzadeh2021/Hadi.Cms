@using Hadi.Cms.Language.Resources

@{
    ViewBag.Title = Strings.Global_SiteStatistic;
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<div class="wrapper wrapper-content animated fadeInRight">
	<div class="row">
		<div class="col-lg-2">
			<div class="widget lazur-bg p-lg text-center">
				<div class="m-b-md">
					<i class="fa fa-line-chart fa-4x"></i>
					<h1 class="m-xs">@ViewBag.AlexaRank</h1>
					<h3 class="font-bold no-margins">
						@Strings.Statistic_AlexaRank
					</h3>
					<small></small>
				</div>
			</div>
		</div>
		<div class="col-lg-2">
			<div class="widget blue-bg p-lg text-center">
				<div class="m-b-md">
					<i class="fa fa-line-chart fa-4x"></i>
					<h1 class="m-xs">@ViewBag.AlexaRankInCountry</h1>
					<h3 class="font-bold no-margins">
						@Strings.Statistic_AlexaRank
					</h3>
					<small>@Strings.Global_InCountry</small>
				</div>
			</div>
		</div>
	</div>
</div>
